---
apiVersion: v1
kind: Service
metadata:
  name: "{{ meta.name }}-redis"
  namespace: "{{ project_name }}"
  labels:
    app: "{{ deployment_type }}-redis"
spec:
  selector:
    app: "{{ deployment_type }}-redis"
  ports:
    - protocol: TCP
      targetPort: 6379
      name: redis-6379
      port: 6379
